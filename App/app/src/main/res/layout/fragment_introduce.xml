<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="VmShareViewModel"
            type="com.project.healingEars.activity.ui.activity.VmShareViewModel" />
    </data>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.project.healingEars.activity.ui.introduce.IntroduceFragment">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="이름"
        android:gravity="center"/>
    <TextView
        android:id="@+id/blank_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@{VmShareViewModel.userVO.userName}"
        android:gravity="center"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="이메일"
        android:gravity="center"/>
    <TextView
        android:id="@+id/blank_nickName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@{VmShareViewModel.userVO.userTel}"
        android:gravity="center"/>



</LinearLayout>
</layout>